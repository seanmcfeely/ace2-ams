    "pinia": "^2.0.13",
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primevue": "^3.12.5",
    "snakecase-keys": "^5.4.1",
    "v-calendar": "^3.0.0-alpha.8",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
